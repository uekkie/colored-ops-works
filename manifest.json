{
  "name": "OpwWorksの色をつける拡張",
  "description": "OpwWorksの色をつける拡張",
  "version": "0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://ap-northeast-1.console.aws.amazon.com/*",
        "https://us-east-1.console.aws.amazon.com/opsworks/*",
        "https://s3.console.aws.amazon.com/*"],
      "js": ["colored-ops-works.js"]
    }
  ]
}